{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "frontendDist": "../dist",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "Modpack Store",
  "mainBinaryName": "dev-alexitoo-modpackstore",
  "version": "0.1.0",
  "identifier": "dev.alexitoo.modpackstore",
  "plugins": {},
  "app": {
    "security": {
      "csp": null,
      "capabilities": [
        "migrated"
      ]
    },
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Modpack Store",
        "width": 960,
        "height": 600,
        "resizable": true,
        "minHeight": 600,
        "minWidth": 960,
        "decorations": false
      }
    ]
  }
}